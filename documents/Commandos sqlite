/*SELECT ID_Proyecto as idProyecto, Pagado as pagado, Proveedor as proveedor, sum(cantidad) as total FROM Compra WHERE Pagado = 'No' GROUP BY ID_Proyecto HAVING total > 50;*/
SELECT ID_Proyecto, Pagado, Proveedor, sum(cantidad) as total FROM Compra WHERE Pagado = 'No' GROUP BY ID_Proyecto HAVING total > 50;

/*SELECT Tipo.Estrato as estrato, Proyecto.Fecha_Inicio as fechaInicio, Tipo.Financiable as financiable, Proyecto.Porcentaje_Cuota_Inicial * 100 AS porcentaje FROM Tipo, Proyecto WHERE Proyecto.ID_Tipo = Tipo.ID_Tipo AND Proyecto.Porcentaje_Cuota_Inicial > 0.2 AND Tipo.Estrato >= 5 AND Tipo.Financiable = 0 ORDER BY FechaInicio;*/
SELECT Tipo.Estrato, Proyecto.Fecha_Inicio AS inicio, Tipo.Financiable, Proyecto.Porcentaje_Cuota_Inicial * 100 FROM Tipo, Proyecto WHERE Proyecto.ID_Tipo = Tipo.ID_Tipo AND Proyecto.Porcentaje_Cuota_Inicial >= 0.3 AND Tipo.Estrato >= 5 AND Tipo.Financiable = 0 ORDER BY inicio;

/*SELECT Proyecto.Clasificacion AS clasificacion, Proyecto.ID_Proyecto AS idProyecto, Compra.pagado AS pagado FROM MaterialConstruccion, Compra, Proyecto WHERE MaterialConstruccion.Importado = 'Si' AND Compra.ID_MaterialConstruccion = MaterialConstruccion.ID_MaterialConstruccion AND Proyecto.ID_Proyecto = Compra.ID_Proyecto AND Compra.Pagado IN ('Parcialmente','No') AND Proyecto.Clasificacion IN ('Apartamento','Apartaestudio') AND Proyecto.Acabados = 'No' GROUP BY Proyecto.ID_Proyecto;*/
SELECT Proyecto.Clasificacion, Proyecto.ID_Proyecto as idProyecto, Compra.pagado FROM MaterialConstruccion, Compra, Proyecto WHERE MaterialConstruccion.Importado = 'Si' AND Compra.ID_MaterialConstruccion = MaterialConstruccion.ID_MaterialConstruccion AND Proyecto.ID_Proyecto = Compra.ID_Proyecto AND Compra.Pagado IN ('Parcialmente','No') AND Proyecto.Clasificacion IN ('Apartamento','Apartaestudio') AND Proyecto.Acabados = 'No' GROUP BY idProyecto;
